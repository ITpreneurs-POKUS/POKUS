{
  "expo": {
    "name": "Pokus",
    "slug": "pokus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/PokusLogo.png",
    "userInterfaceStyle": "light",
    "notification":{
      "icon":"./assets/PokusLogo.png",
      "color":"#050A30"
    },
    "splash": {
      "image": "./assets/SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#050A30",
      "android": {
        "fadeOutDuration": 3000,
        "fadeOutMode": "delay"
      }
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesAudio": true
      }
    },
    "android": {
      "icon": "./assets/notifPokusLogo.png",
      "color":"#050A30",
      "adaptiveIcon": {
        "useNextNotificationsApi": true,
        "foregroundImage": "./assets/PokusLogo.png",
        "backgroundColor": "#050A30",
        "permissions": ["NOTIFICATIONS", "READ_EXTERNAL_STORAGE"],
        "splash": {
          "image": "./assets/SplashScreen.png",
          "resizeMode": "contain",
          "backgroundColor": "#050A30",
          "fadeOutDuration": 3000,
          "fadeOutMode": "delay"
        }
      },
      "package": "com.pokus.app",
      "notification": {
        "androidMode": "default",
        "sound": true,
        "icon": "./assets/notifPokusLogo.png",
        "color":"#050A30"
      },
      "config": {
        "googleServicesFile": "./google-services.json",
        "usesAudio": true
      },
      "plugins": [
        [
          "expo-background-fetch",
          {
            "taskName": "TIMER_TASK",
            "options": {
              "minimumInterval": 15
            }
          }
        ]
      ]
    },
    "web": {
      "allowNotifications": true,
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8746ef83-f341-4549-846d-8d8a78238008"
      }
    },
    "owner": "khenpaler101"
  }
}
